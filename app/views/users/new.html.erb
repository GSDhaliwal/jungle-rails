<h1>Signup!</h1>

<%= form_for :user, url: '/users' do |f| %>

  First Name: 
  <br>
  <%= f.text_field :firstName %>
  <br>
  Last Name:
  <br>
  <%= f.text_field :lastName %>
  <br>
  Email:
  <br>
  <%= f.text_field :email %>
  <br>
  Password:
  <br>
  <%= f.password_field :password %>
  <br>
  Password Confirmation:
  <br>
  <%= f.password_field :password_confirmation %>
  <br>
  <br>
  <%= f.submit "Submit" %>

<% end %>